{
  "name": "it-asset-management-frontend",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "preview:e2e": "node ./scripts/e2e-server.mjs",
    "test:e2e:dev": "playwright test -c playwright.dev.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "start:backend": "cd ../backend && mvn spring-boot:run",
    "start:both": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "health:frontend": "curl -sS --http1.1 -o /dev/null -w '%{http_code}\\n' http://localhost:3001",
    "health:backend": "curl -sS --http1.1 -o /dev/null -w '%{http_code}\\n' http://localhost:8080/api/v1/actuator/health",
    "gen:api": "openapi-typescript http://localhost:8080/api/v1/api-docs --output src/generated/api-types.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@tanstack/react-query": "^5.56.2",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.8",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "openai": "^4.67.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.9.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.2",
    "express-rate-limit": "^8.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/dexie": "^1.3.32",
    "@types/node": "^22.8.4",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "openapi-typescript": "^7.4.3",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7"
  }
}
